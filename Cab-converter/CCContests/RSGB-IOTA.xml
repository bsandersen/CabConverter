<Contest name="RSGB IOTA" sponsor="RSGB" shortName="RSGB-IOTA">
	<UI>
		<Location prompt="Location:" type="list">
			<Item>Island station</Item>
			<Item>World station</Item>
		</Location>	

		<OpCount prompt="Operators:" type="list">
			<Item>SINGLE-OP</Item>
			<Item>MULTI-OP</Item>
		</OpCount>

		<CategoryAssisted prompt="Assited?" type="list">
			<Item>ASSISTED</Item>
			<Item>UNASSISTED</Item>
		</CategoryAssisted>

		<DXpedition prompt="DXpedition?" type="list">
			<Item>EXPEDITION</Item>
			<Item>NON-DXPEDITION</Item>
		</DXpedition>

		<CategoryMode prompt="Mode:" type="list">
			<Item>CW</Item>
			<Item>SSB</Item>
			<Item>MIXED</Item>
		</CategoryMode>

		<CategoryPower prompt="Category power:" type="list">
			<Item>HIGH</Item>
			<Item>LOW</Item>
			<Item>QRP</Item>
		</CategoryPower>

		<OperatingTime prompt="Time:" type="list">
			<Item>12-HOURS</Item>
			<Item>24-HOURS</Item>
		</OperatingTime>
	
		<IslandName prompt="Island Name:" type="text"/>
	
		<Operators prompt="Operators:" type="text"/>

		<RecipeVersion prompt="Recipe version:" type="list">
			<Item>contest recipe version 1.0, by NE1RD</Item>
		</RecipeVersion>
	</UI>
			
	<Cabrillo>
		<Line><Text>START-OF-LOG: 2.0</Text></Line>
		<Line><Text>CONTEST: RSGB-IOTA</Text></Line>
		<Line><Text>CALLSIGN: </Text> <MyCallsign/> </Line>
		<Line><Text>OPERATORS: </Text> <Operators/> </Line>
		<Line><Text>CATEGORY: </Text> <OpCount/> 
			<Text> ALL </Text> <CategoryPower/>
			<Text> </Text> <CategoryMode/> </Line>
		<Line><Text>CATEGORY-ASSISTED: </Text> <CategoryAssisted/> </Line>	
		<Line><Text>CATEGORY-DXPEDITION: </Text> <DXpedition/> </Line>
		<Line><Text>CATEGORY-TIME: </Text> <OperatingTime/> </Line>
		<Line><Text>IOTA-ISLAND-NAME: </Text> <IslandName/> </Line>
		<Line><Text>NAME: </Text><Name/></Line>
		<Line><Text>ADDRESS: </Text><Address1/></Line>
		<Line><Text>ADDRESS: </Text><Address2/></Line>
		<Line><Text>ADDRESS: </Text><City/></Line>
		<Line><Text>ADDRESS: </Text><State/></Line>
		<Line><Text>ADDRESS: </Text><PostalCode/></Line>
		<Line><Text>ADDRESS: </Text><Country/></Line>					
		<Line><Text>ADDRESS: </Text><Email/></Line>		
		<Line><Text>CREATED-BY: </Text><Version/>
			<Text>, </Text> <RecipeVersion/> </Line>
		<QSO>
			<Text>QSO:</Text>
			<Frequency/>
			<Mode/>
			<Date/>
			<Time/>
			<MyCallsign/>
			<RSTs/>
			<STXn/>
			<STX ifEmpty="-----"/>
			<TheirCallsign/>
			<RSTr/>
			<SRXn/>
			<SRX ifEmpty="-----"/>
		</QSO>
		<Line><Text>END-OF-LOG:</Text></Line>
	</Cabrillo>
</Contest>
